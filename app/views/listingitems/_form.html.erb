
<%= simple_form_for(@listingitem) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :iframelink %>
    <%= f.input :mileage %>
    <%= f.input :bodytype %>
    <%= f.input :doors %>
    <%= f.input :engine %>
    <%= f.input :transmission %>
    <%= f.input :drivetrain %>
    <%= f.input :exteriorcolor %>
    <%= f.input :interiorcolor %>
    <%= f.input :condition %>
    <%= f.input :feul %>
    <%= f.input :title %>
    <%= f.input :slug %>
    <%= f.input :listingcat %>
    <%= f.input :references %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
