<p id="notice"><%= notice %></p>

<h1>Leaveapps</h1>

<table>
  <thead>
    <tr>
      <th>Leave package</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Military</th>
      <th>Status</th>
      <th>Verified</th>
      <th>Slug</th>
      <th>Message</th>
      <th>Date</th>
      <th>Price</th>
      <th>Duration</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leaveapps.each do |leaveapp| %>
      <tr>
        <td><%= leaveapp.leave_package %></td>
        <td><%= leaveapp.name %></td>
        <td><%= leaveapp.email %></td>
        <td><%= leaveapp.phone %></td>
        <td><%= leaveapp.military_id %></td>
        <td><%= leaveapp.status %></td>
        <td><%= leaveapp.verified %></td>
        <td><%= leaveapp.slug %></td>
        <td><%= leaveapp.message %></td>
        <td><%= leaveapp.date %></td>
        <td><%= leaveapp.price %></td>
        <td><%= leaveapp.duration %></td>
        <td><%= leaveapp.user_id %></td>
        <td><%= link_to 'Show', leaveapp %></td>
        <td><%= link_to 'Edit', edit_leaveapp_path(leaveapp) %></td>
        <td><%= link_to 'Destroy', leaveapp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Leaveapp', new_leaveapp_path %>
