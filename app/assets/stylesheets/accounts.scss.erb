@import "app.min";
@font-face {
	font-family: 'feather';
	src:url('<%= asset_path ('feathere8ab.eot') %>');
	src:url('<%= asset_path ('feathere8ab.eot') %>') format('embedded-opentype'),
		url(' <%= asset_path ('feathere8ab.woff') %>') format('woff'),
		url(' <%= asset_path ('feathere8ab.ttf') %>') format('truetype'),
		url('<%= asset_path ('feathere8ab.svg') %>') format('svg');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'Line Awesome Brands';
	src:url('https://cdnjs.cloudflare.com/ajax/libs/simple-line-icons/2.5.5/fonts/Simple-Line-Icons.eot');
	src:url('https://cdnjs.cloudflare.com/ajax/libs/simple-line-icons/2.5.5/fonts/Simple-Line-Icons.eot') format('embedded-opentype'),
		url(' <%= asset_path ('la-regular-400.woff') %>') format('woff'),
		url('https://cdnjs.cloudflare.com/ajax/libs/simple-line-icons/2.5.5/fonts/Simple-Line-Icons.ttf') format('truetype'),
		url('https://cdnjs.cloudflare.com/ajax/libs/simple-line-icons/2.5.5/fonts/Simple-Line-Icons.svg') format('svg');
	font-weight: normal;
	font-style: normal;
}


