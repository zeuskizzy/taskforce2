  @import url('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css');


@import "bootstrap.min";
@import "animate.min";
@import "meanmenu";
@import "boxicons.min";
@import "flaticon";
@import "odometer.min";
@import "owl.carousel.min";
@import "owl.theme.default.min";
@import "magnific-popup.min";
@import "nice-select.min";
@import "slick.min";
@import "slick-theme.min";
@import "style";
@import "responsive";
@import "dark";



@font-face {
	font-family:'boxicons';font-weight:400;font-style:normal;
	src:url('<%= asset_path ('boxicons.eot') %>');
	src:url('<%= asset_path ('boxicons.eot') %>') format('embedded-opentype'),
		url(' <%= asset_path ('boxicons.woff') %>') format('woff'),
		url(' <%= asset_path ('boxicons.ttf') %>') format('truetype'),
		url('<%= asset_path ('boxiconsd41d.svg') %>') format('svg');

}

@font-face {
	font-family: "Flaticon";
	src: url('<%= asset_path ('Flaticon.eot')%>');
	src: url('<%= asset_path ('Flaticond41d.eot?#iefix')%>') format("embedded-opentype"),
	url('<%= asset_path ('Flaticon.woff2')%>') format("woff2"),
	url('<%= asset_path ('Flaticon.woff')%>') format("woff"),
	url('<%= asset_path ('Flaticon.ttf')%>') format("truetype"),
	url('<%= asset_path ('Flaticon.svg#Flaticon')%>') format("svg");
	font-weight: normal;
	font-style: normal;
}
@media screen and (-webkit-min-device-pixel-ratio:0) {
	@font-face {
		font-family: "Flaticon";
		src: url('<%= asset_path ('Flaticon.svg#Flaticon')%>') format("svg");
	}
}
